    "# 파일명+.+확장자를 나타내는 정규식\n",
    "p =re.compile('.*[.].*$')\n",
    "list = ['foo.exe', 'autoexec.bat', 'sendmail.cf']\n",
    "for i in list:\n",
    "    if p.match(i):\n",
